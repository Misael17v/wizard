<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="wizard_prueba" model="ir.ui.view">
            <field name="name">wizard_prueba</field>
            <field name="model">wizard</field>
            <field name="arch" type="xml">
               <form string="introdoo">
                  <group>
                    <group name="company_dates" string="general information">

                    </group>
                    <group>
                        <field name="date_from"/>
                        <field name="date_to"/>
                    </group>
                    <group>
                        <field name="name"/>
                        <field name="user_id"/>
                    </group>
                  </group>
                  <footer>
                    <button name="check_report" string="Print" type="object" default_focus="1" class="oe_highlight" />
                    <button string="Cancel" class="btn-secondary" special="cancel"/>
                  </footer>
               </form>
            </field>
        </record>

        <record id="action_menu_wizard" model="ir.actions.act_window">
            <field name="name">wizard_menu</field>
            <field name="res_model">wizard</field>
            
            
            <field name="view_mode">form</field>
            <field name="view_id" ref="wizard_prueba"/>
            <field name="target">new</field>
        </record>

        <menuitem id="menu_introdoo"
         name="Menu "
         parent="my_library.library_base_menu"
         sequence="10"/>

       <menuitem id="submwnu_introdoo"
       name="pfd"
       sequence="3"
       action="action_menu_wizard"
       parent="my_library.menu_introdoo" /> 
    </data>
</odoo>